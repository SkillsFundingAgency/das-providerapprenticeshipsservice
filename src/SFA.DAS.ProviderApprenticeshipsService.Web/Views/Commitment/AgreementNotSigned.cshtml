@model SFA.DAS.ProviderApprenticeshipsService.Web.Models.AgreementNotSignedViewModel

@{
    ViewBag.Title = "Agreement not signed";
    ViewBag.PageId = "agreement-not-signed";
}

<div class="grid-row">
    <div class="column-two-thirds">
        <h1 class="heading-xlarge">Agreement not signed</h1>
        <div class="notice">
            <i class="icon icon-important">
                <span class="visually-hidden">Warning</span>
            </i>
           
                <strong class="bold-small">
                    Your organisation must have a signed Skills Funding Agency apprenticeship agreement before you can approve cohorts. 
                </strong>
        
        </div>
       <p><a class="button" href="@Model.ReviewAgreementUrl" target="_blank" aria-label="Review your agreement">Review your agreement</a>
        <p><a href="@Model.RequestListUrl">Continue to cohort</a></p>
    </div>
</div>
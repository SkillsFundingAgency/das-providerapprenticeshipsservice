<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />

    <sessionState mode="Custom" customProvider="RedisSessionProvider" xdt:Transform="Replace">
      <providers>
        <add type="SFA.DAS.Web.Redis.RedisSessionStateProvider" name="RedisSessionProvider" connectionString="RedisSessionProviderConnectionString" />
      </providers>
    </sessionState>
  </system.web>

  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd" autoReload="true" throwExceptions="true" xdt:Transform="RemoveAttributes(internalLogLevel,internalLogFile)">
    <rules>
      <logger minlevel="Trace" writeTo="Redis" xdt:Transform="SetAttributes" />
      <logger name="*" minlevel="Trace" writeTo="aiTarget" xdt:Transform="Insert"/>
    </rules>
  </nlog>
</configuration>

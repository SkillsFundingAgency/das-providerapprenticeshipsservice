<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SFA.DAS.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6" upgradeDomainCount="5">
  <WebRole name="SFA.DAS.ProviderApprenticeshipsService.Web" vmsize="Standard_D2_v2">
    <Sites>
      <Site name="Web">
        <Bindings>
         <Binding name="Httpsin" endpointName="Httpsin" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
      <Setting name="EnvironmentName" />
      <Setting name="ConfigurationStorageConnectionString" />
      <Setting name="LoggingRedisConnectionString"/>
      <Setting name="LoggingRedisKey"/>
      <Setting name="RedisConnection" />
      <Setting name="IdamsRealm"/>
      <Setting name="IdamsADFSMetadata"/>
      <Setting name="InstrumentationKey" />
      <Setting name="RedisSessionProviderConnectionString" />
      <Setting name="CurrentTime" />
      <Setting name="FeatureToggle.ManageReservations" />
      <Setting name="UseStubProviderRelationships" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Httpsin" protocol="https" port="443" certificate="WebSslCert" loadBalancer="das-prd-pas-ilb"/>
    </Endpoints>
    <Certificates>
      <Certificate name="WebSslCert" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
  </WebRole>
</ServiceDefinition>
